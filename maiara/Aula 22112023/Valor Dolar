// Online C compiler to run C program online
#include <stdio.h>

int main() {
    float valorCarteira, valorDolar, dolarComprar;
    
    printf("Digite quanto dinhiero hรก na carteira:R$ ");
    scanf("%f",&valorCarteira);
    
    valorDolar = 4.90;
    
    dolarComprar = valorCarteira / valorDolar;

    printf("Pode comprar: %f",dolarComprar);
    
    
    return 0;
}
